<Domain id="stereotype" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="name" name="Name" >
		<Str/>
	</Attribute>
    <Attribute id="scope" name="Scope" >
        <Enum default="Private">
			<Value>Private</Value>
	        <Value>Protected</Value>
	        <Value>Public</Value>
		</Enum>
    </Attribute>
    <Attribute id="abstract" name="Abstract" >
		<Bool default="False"/>
	</Attribute>
    <Attribute id="tags" name="Tags" >
		<List>
        	<Domain>
        	    <Attribute id="name" name="Name" >
					<Str/>
				</Attribute>
        	    <Attribute id="type" name="Type" >
					<Enum default="str">
                        <Value>str</Value>
                        <Value>int</Value>
                        <Value>float</Value>
                        <Value>bool</Value>
                        <Value>text</Value>
                    </Enum>
				</Attribute>
        	    <Attribute id="default" name="Default" >
					<Str/>
				</Attribute>
        	    <Attribute id="note" name="Note" >
					<Text/>
				</Attribute>
        	</Domain>
		</List>
    </Attribute>
    <Attribute id="note" name="Note" >
		<Text/>
	</Attribute>
</Domain>