<Domain id="package" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="name" name="Name" >
		<Str/>
	</Attribute>
    <Attribute id="stereotype" name="Stereotype" >
		<Str/>
	</Attribute>
    <Attribute id="scope" name="Scope" >
        <Enum default="Private">
			<Value>Private</Value>
	        <Value>Protected</Value>
	        <Value>Public</Value>
		</Enum>
    </Attribute>
    <Attribute id="applied_profiles" name="Applied profiles" hidden="true">
        <List>
            <Domain>
                <Attribute id="package_id" name="Package Identifier">
                    <Str />
                </Attribute>
                <Attribute id="profile_name" name="Profile name">
                    <Str />
                </Attribute>
                <Attribute id="modification_bundle" name="Modification bundle">
                    <Str />
                </Attribute>
            </Domain>
        </List>
    </Attribute>
    <Attribute id="abstract" name="Abstract" >
		<Bool default="False"/>
	</Attribute>
    <Attribute id="note" name="Note" >
		<Text/>
	</Attribute>
</Domain>