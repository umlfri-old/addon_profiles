<Domain id="stereotype" xmlns="http://umlfri.kst.fri.uniza.sk/xmlschema/metamodel.xsd" >
    <Attribute id="name" name="Name" >
		<Str/>
	</Attribute>
    <Attribute id="icon" name="Icon">
        <Enum>
            <Value>None</Value>
            <Value>Actor</Value>
            <Value>Wizard</Value>
            <Value>Map Location</Value>
            <Value>Loading</Value>
            <Value>Question Mark</Value>
            <Value>Settings</Value>
        </Enum>
    </Attribute>
    <Attribute id="tags" name="Tags" >
		<List>
        	<Domain>
        	    <Attribute id="name" name="Name" >
					<Str/>
				</Attribute>
        	    <Attribute id="type" name="Type" >
					<Enum default="str">
                        <Value>str</Value>
                        <Value>int</Value>
                        <Value>float</Value>
                        <Value>bool</Value>
                        <Value>text</Value>
                    </Enum>
				</Attribute>
        	    <Attribute id="note" name="Note" >
					<Text/>
				</Attribute>
        	</Domain>
		</List>
    </Attribute>
    <Attribute id="note" name="Note" >
		<Text/>
	</Attribute>
</Domain>